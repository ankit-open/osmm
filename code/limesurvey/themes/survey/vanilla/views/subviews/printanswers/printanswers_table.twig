
{% if (aSurveyInfo.printPdf != 1) %} 
    <div class="row">
        <div class="col-sm-12">
            <h2>{{ gT("Survey name (ID):") }} {{aSurveyInfo.surveyls_title}} ({{aSurveyInfo.sid}})</h2>
        </div>
    </div>
	<script type="text/javascript" src="{{chartJS}}"></script>
	<script type="text/javascript" src="{{chartApexJS}}"></script>
	<div class="container public-stats">
		<div class="row">
			<div class="col-sm-12">
				<div id='statsContainer'>
					<div class="public-stats__content">
					{{output}}
					</div>
				</div>
				<input type='hidden' class='hidemebutton'/>
			</div>
		</div>
	</div>
{% endif %} 

{% for groupid,group in aSurveyInfo.groupArray %}
    {% if (aSurveyInfo.printPdf == 1) %} 
    <br pagebreak="true" /> 
    {% endif %}
    <groupsection class="groupSection"> 
       <!-- Group Name -->
        <div class="row">
		<!-- Question Data -->
        </div>
    </groupsection> 
{% endfor %}








